[core]
  excludesfile = /Users/SeungUk/dotfiles/.gitignore_global
  precomposeunicode = true
  quotepath = false
  pager = delta
[delta]
  side-by-side = true
  line-numbers-left-format = ""
  line-numbers-right-format = "â”‚ "
  syntax-theme = Monokai Extended
[interactive]
  diffFilter = delta --color-only
